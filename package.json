{
  "name": "next-cowsayify",
  "version": "1.0.0",
  "scripts": {
    "1": "yarn rimraf .next && yarn concurrently \"yarn dev\" \"yarn 2\" \"yarn sidecar\"",
    "2": "yarn css-watch",
    "sidecar": "yarn barrel-rider-sidecar --remove --watch --src components api-lib lib common",
    "css-watch": "yarn tsm  \"**/*.module.scss\" --ignore \"**/node_modules\" --watch",
    "css": "yarn tsm  \"**/*.module.scss\" --ignore \"**/node_modules\"",
    "dev": "next -p 3001",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc"
  },
  "dependencies": {
    "aws-sdk": "2.1202.0",
    "canvas": "2.9.3",
    "chroma-js": "2.4.2",
    "cowsay-browser": "^1.1.8",
    "fast-random": "^2.0.4",
    "isomorphic-unfetch": "3.1.0",
    "jotai": "^1.8.1",
    "lodash": "4.17.21",
    "luxon": "3.0.1",
    "moment": "2.29.4",
    "nanoid": "4.0.0",
    "next": "12.2.5",
    "node-sass": "7.0.1",
    "ramda": "0.28.0",
    "ramda-adjunct": "3.2.0",
    "react": "18.2.0",
    "react-aria-menubutton": "7.0.3",
    "react-dom": "18.2.0",
    "sass": "^1.54.5",
    "slugify": "1.6.5",
    "tailwindcss": "^3.1.8",
    "victor": "^1.1.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.30",
    "@fortawesome/free-regular-svg-icons": "5.14.0",
    "@fortawesome/free-solid-svg-icons": "5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/forms": "^0.5.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/lodash": "4.14.184",
    "@types/luxon": "3.0.0",
    "@types/moment": "^2.13.0",
    "@types/nanoid": "3.0.0",
    "@types/node": "18.7.13",
    "@types/ramda": "0.28.15",
    "@types/react": "18.0.17",
    "@types/react-aria-menubutton": "6.2.9",
    "@types/react-dom": "18.0.6",
    "@types/victor": "^1.1.0",
    "@zeit/next-sass": "^1.0.1",
    "autoprefixer": "^10.4.8",
    "barrel-rider-sidecar": "1.1.7",
    "concurrently": "7.3.0",
    "husky": "4.3.0",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "rimraf": "^3.0.2",
